@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

@Html.ActionLink("Add a new student", "Create")

<table>
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Phone</th>
        <th>SID</th>
    </tr>
    @foreach (var item in ViewBag.stu)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.Phone</td>
            <td>@item.StudentNumber</td>
        </tr>
    }
</table>